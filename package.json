    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@originjs/vite-plugin-federation": "^1.2.1",
