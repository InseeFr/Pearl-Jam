{
  "id": "${APP_ID}",
  "instances": 1,
  "cpus": 0.1,
  "mem": 1024,
  "disk": 0,
  "gpus": 0,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "${IMAGE_JS}",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "default"
        }
      ],
      "privileged": false,
      "forcePullImage": true
    }
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "protocol": "MESOS_TCP",
      "ipProtocol": "IPv4",
      "timeoutSeconds": 20,
      "delaySeconds": 15
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_REDIRECT_TO_HTTPS": "true",
    "HAPROXY_0_VHOST": "localhost-${BRANCHNAME}-pearl-sic.dev.innovation.insee.eu",
    "HAPROXY_0_ENABLED": "true"
  },
  "requirePorts": true,
  "constraints": [["type", "IS", "boss"]]
}
