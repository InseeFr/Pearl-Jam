    "preview": "vite preview",
    "format": "prettier --write .",
    "test": "vitest",
    "lint:fix": "prettier --write  \"**/*.{js,jsx,ts,tsx,css,scss,html,json}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
